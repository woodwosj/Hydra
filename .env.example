# Path to the Codex CLI executable. Defaults to searching PATH when unset.
CODEX_PATH=

# Default model identifier passed to Codex when spawning agents (e.g., gpt-4.1).
CODEX_DEFAULT_MODEL=

# Filesystem path where Hydra MCP should persist Chroma collections.
CHROMA_PERSIST_PATH=./storage/chroma

# Optional logging level override (DEBUG, INFO, WARNING, ERROR).
HYDRA_LOG_LEVEL=INFO
